{
  "name": "professional-portfolio-firebase",
  "version": "1.0.0",
  "description": "Firebase setup for professional portfolio website",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "preview": "cd client && npm run preview",
    "firebase": "firebase",
    "emulators": "firebase emulators:start",
    "setup:firestore": "node setup-firestore.js",
    "deploy:rules": "firebase deploy --only firestore,storage",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:all": "firebase deploy",
    "postinstall": "cd client && npm install"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.7",
    "@radix-ui/react-toast": "^1.2.10",
    "@tailwindcss/postcss": "^4.1.4",
    "autoprefixer": "^10.4.21",
    "canvas": "^3.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase-admin": "^11.11.1",
    "framer-motion": "^12.7.4",
    "i18next": "^25.0.0",
    "i18next-browser-languagedetector": "^8.0.5",
    "lucide-react": "^0.501.0",
    "postcss": "^8.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.55.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "wouter": "^3.7.0"
  },
  "devDependencies": {
    "firebase-tools": "^14.2.0"
  }
}
